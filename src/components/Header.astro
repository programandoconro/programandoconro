---
import Download from './Download.astro'
import Language from './Language.astro';
import BlogIcon from './BlogIcon.astro'

---
<header>
    <Download id="download-file-header" />
    <BlogIcon />
    <Language />

</header>

<style>
    header {
        position: relative;
        display: flex;
        justify-content: space-between;
    }
  @media only screen and (min-width: 1400px) {
    header {
        z-index: 2;
        left: 0;

    }
  }


</style>
